{
  "tasks": [
    {
      "type": "shell",
      "label": "CF-Test",
      "command": "clear&&cf test ${fileBasename}",
      "options": {
        "cwd": "${fileDirname}"
      },
      "problemMatcher": []
    },
    {
      "type": "shell",
      "label": "CF-Submit",
      "command": "clear&&cf submit ${fileBasename}",
      "options": {
        "cwd": "${fileDirname}"
      },
      "problemMatcher": []
    },
    {
      "type": "shell",
      "label": "CF-Menu",
      "command": "clear&&cf ${input:CFFunction}",
      "options": {
        "cwd": "${fileDirname}"
      },
      "problemMatcher": []
    },
    {
      "type": "shell",
      "label": "CF-MenuContest",
      "command": "clear&&cf ${input:CFFunctionMin} ${input:CFContestID}",
      "options": {
        "cwd": "${fileDirname}"
      },
      "problemMatcher": []
    },

    {
      "type": "cppbuild",
      "label": "C/C++: g++-9 build active file",
      "command": "/usr/bin/g++-9",
      "args": [
        "-fdiagnostics-color=always",
        "-g",
        "${file}",
        "-o",
        "${fileDirname}/${fileBasenameNoExtension}"
      ],
      "options": {
        "cwd": "${fileDirname}"
      },
      "problemMatcher": ["$gcc"],
      "group": {
        "kind": "build",
        "isDefault": true
      },
      "detail": "Task generated by Debugger."
    }
  ],
  "inputs": [
    {
      "id": "CFContestID",
      "description": "Contest ID",
      "type": "promptString"
    },
    {
      "id": "CFFunction",
      "description": "Select operation",
      "type": "pickString",
      "options": [
        "list",
        "watch",
        "parse",
        "test",
        "submit",
        "open",
        "stand",
        "sid",
        "pull",
        "upgrade"
      ]
    },
    {
      "id": "CFFunctionMin",
      "description": "Select operation",
      "type": "pickString",
      "options": ["race", "parse"]
    }
  ],
  "version": "2.0.0"
}
